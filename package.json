{
  "name": "medusa-plugin-product-reviews",
  "version": "0.0.23",
  "description": "A plugin that enables customer product reviews for Medusa v2.13+.",
  "author": "Lambda Curry <team@lambdacurry.dev>",
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/leonardozhe/medusa-plugin-product-reviews.git"
  },
  "keywords": [
    "medusa",
    "ecommerce",
    "product-reviews",
    "headless"
  ],
  "scripts": {
    "build": "rm -rf dist && tsc --build",
    "watch": "tsc --build --watch"
  },
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./admin": {
      "types": "./dist/admin/index.d.ts",
      "default": "./dist/admin/index.js"
    }
  },
  "dependencies": {
    "@medusajs/framework": "^2.13.0",
    "@medusajs/ui": "^4.0.0",
    "multer": "^1.4.5-lts.1",
    "cors": "^2.8.5"
  },
  "peerDependencies": {
    "@medusajs/medusa": "^2.13.0",
    "@medusajs/framework": "^2.13.0",
    "@medusajs/ui": "^4.0.0"
  },
  "devDependencies": {
    "@medusajs/medusa": "^2.13.0",
    "@medusajs/framework": "^2.13.0",
    "@types/express": "^4.17.21",
    "@types/node": "^20.0.0",
    "typescript": "^5.3.0"
  }
}
